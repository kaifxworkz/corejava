public class Test2{
public static void main(String[] args){
run();
working();
 int returnint=marks();
  System.out.println(returnint);
  short returnshort=sub();
  System.out.println(returnshort);
  boolean returnboolean=example();
  System.out.println(returnboolean);
  char returnchar=alpha();
  System.out.println(returnchar);
  float returnfloat=decimal();
  System.out.println(returnfloat);
  long returnlong=num();
  System.out.println(returnlong);
  byte returnbyte=index();
  System.out.println(returnbyte);
  double returndouble=deci();
  System.out.println(returndouble);
}
 

static void  speaking ()
 {
 System.out.println("dog is speaking");
 }

	


    static void working()
 	 {
		 System.out.println(" sam is working properly");
	 }
 
 
 
      static int marks()
 {
	 int x=10;
	 int y=20;
	 int z=a+b;
	 return c;
	
	 
 }

 static short sub()
 {
	 short a=157;
	 
	 
	 return a;
	 
 }




      static  boolean  example()
 {
	 boolean a=true;
	 return true;
 }
  static char alpha()
  {
  char e='A';
  return e;
  
  }  



  
        static float decimal()
  {
	  float value=5.6f;
	  return value;
  }




    static long num()
  {
	  long no=12000;
	  return no;
  }





static byte index()
{
	byte i=9;
	return i;
}
static double deci()
{
	double d=18.567;
	return d;
}
